{
  "name": "@moonmoonbrothers/flutterjs",
  "version": "1.1.3",
  "description": "this is svg framework(virtual dom, state management, even calculation of layout)",
  "main": "src/index.ts",
  "devDependencies": {
    "cp": "^0.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "popmotion": "^11.0.5",
    "short-unique-id": "^4.4.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "esm"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup --dts",
    "prepackage": "npm run build",
    "package": "cp package.json dist/package.json && cp README.md dist/README.md && cd dist && npm pkg delete main && npm pkg set types='./index.d.ts' && npm pkg set type='module' &&  npm pkg set exports='{\".\":  \"./index.mjs\"}' --json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonmoonbrothers/uglychart.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moonmoonbrothers/uglychart/issues"
  },
  "homepage": "https://637c944f61fbd5154eb3e034-esfdvuwvfq.chromatic.com/?path=/docs/introduction--docs"
}
